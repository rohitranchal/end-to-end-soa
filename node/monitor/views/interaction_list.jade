html(lang="en")
	include header
	body
		include nav

		div(class="container")
			div(class="panel panel-default text-center")
				h2 Service Interactions
			div(class="panel panel-default")
				div(class="panel-body")
					div(class="row")
						div(class="col-md-2")
							strong From
						div(class="col-md-2")
							strong To
						div(class="col-md-2")
							strong Start
						div(class="col-md-2")
							strong End
						div(class="col-md-4")
							strong Timestamp
					for entry in entries
						div(class="row")
							div(class="col-md-2")=entry.from_service
							div(class="col-md-2")=entry.to_service
							div(class="col-md-2")=entry.start
							div(class="col-md-2")=entry.end
							div(class="col-md-4")=entry.ts
